{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "restoreTerminals.terminals": [
    {
      "splitTerminals": [
        {
          "name": "next",
          "commands": ["cd web", "yarn dev"]
        },
        {
          "name": "cmd",
          "commands": ["cd web"]
        }
      ]
    },
    {
      "splitTerminals": [
        {
          "name": "expo",
          "commands": ["cd mobile", "npx expo start --clear"]
        },
        {
          "name": "cmd",
          "commands": ["cd mobile"]
        }
      ]
    },
    {
      "splitTerminals": [
        {
          "name": "api",
          "commands": ["cd api", "yarn dev"]
        },
        {
          "name": "cmd",
          "commands": ["cd api"]
        }
      ]
    },
    {
      "splitTerminals": [
        {
          "name": "claude",
          "commands": ["claude"]
        },
        {
          "name": "cmd",
          "commands": []
        }
      ]
    }
  ],
  "cSpell.words": ["dayjs", "inngest", "rewised", "timeouted", "youwise"],
  "files.exclude": {
    "api/build": true
  },
  /* Added by expo by default: */
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.organizeImports": "explicit",
    "source.sortMembers": "explicit"
  }
}
