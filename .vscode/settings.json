{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "restoreTerminals.terminals": [
    {
      "splitTerminals": [
        {
          "name": "next",
          "commands": ["cd next", "npm run dev"]
        },
        {
          "name": "expo",
          "commands": ["cd native", " npx expo start --clear"]
        },
        {
          "name": "cmd",
          "commands": []
        }
      ]
    }
  ],
  "cSpell.words": ["dayjs", "inngest", "timeouted"],
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma"
  },
  "files.exclude": {
    "native/android": true,
    "native/ios": true
  },
  "sqltools.connections": [
    {
      "previewLimit": 50,
      "server": "localhost",
      "port": 5433,
      "driver": "PostgreSQL",
      "name": "Local",
      "database": "youwise",
      "username": "postgres",
      "password": "postgres"
    }
  ],
  "svg.preview.background": "editor"
}
