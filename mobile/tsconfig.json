{
  "extends": ["expo/tsconfig.base", "../tsconfig.base.json"],
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts"
  ],
  "exclude": ["node_modules", "../node_modules"],
  "references": [{ "path": "../api" }]
}
